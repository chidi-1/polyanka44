Lead.prototype={initTimers:function(timers,isUpdate){log('initTimers(',timers,')','this.timers:',this.timers,'isUpdate:',isUpdate);if(!this.timers||!isUpdate){this.timers={};}
if(!timers){return;}
for(var leadScenarioId in timers){if(timers.hasOwnProperty(leadScenarioId)){this.timers[leadScenarioId]=g.setTimeout(u.bind(this.onTimerUp,this,[leadScenarioId]),timers[leadScenarioId]*1000);}}},initActions:function(actions){log('initActions(',actions,')');actions&&u.arrayForEach(actions,this.doAction,this);},refreshTimers:function(timers,isUpdate){this.cleanupTimers();this.initTimers(timers,isUpdate);},cleanupTimers:function(){for(var leadScenarioId in this.timers){if(this.timers.hasOwnProperty(leadScenarioId)){g.clearTimeout(this.timers[leadScenarioId]);}}},onTimerUp:function(leadScenarioId){g.Comagic.onLeadTimerUp(leadScenarioId);},bindBusHandlers:function(){this.eventBus=uLiB.util.EventBus;if(!this.eventBus){return;}
this.eventBus.on('rpc:init_lead',this.onRpcInitLead,this);this.eventBus.on('rpc:refresh_timers',this.onRpcRefreshTimers,this);this.eventBus.on('rpc:show_lead_form',this.onRpcShowLeadForm,this);this.eventBus.on('rpc:hide_lead_form',this.onRpcHideLeadForm,this);this.eventBus.on('ulib:event',this.onUlibEvent,this);},onRpcInitLead:function(server,params){this.registerForms();this.initTimers(params.timers);this.initActions(params.actions);},onRpcRefreshTimers:function(server,params){this.refreshTimers(params.timers,params.update);},onRpcShowLeadForm:function(server,params){this.doAction(params);},onRpcHideLeadForm:function(server,params){this.hideForm(params);},doAction:function(action,index,actions){if(action.action_type==='auto_invite'){return;}
action.settings.is_show_on_close?this.trackMouseForShowForm(action,true):this.showForm(action,true)},showForm:function(params){log('.showForm(',params,')');var me=this;this.registerForms();params.settings.log_id=params.log_id;var form=this.getForm(params);!form.isModal&&setTimeout(function(){me.formDefaults.is_preview?form.getEl().show():form.show(true);},150);return form;},getForm:function(params){var form=this.forms[params.log_id];if(!form){form=this.forms[params.log_id]=this.makeForm(params);}
return form;},makeForm:function(params){var _comagicWebUrl=this.comagic_web_url||ComagicWidget.settings.webHost();if(u.arrayIndexOf(['personal_form','call_generator'],params.action_type)!==-1){u.addLeadgenCssLink({comagic_web_url:_comagicWebUrl});}
this.registerForms();return ul.cmp.ComponentManager.create(u.apply({ctype:params.action_type,is_lead_gen_preview:this.is_lead_gen_preview,is_preview:this.is_lead_gen_preview,eventBus:this.eventBus,comagic_web_url:_comagicWebUrl}),u.apply(params.settings,this.formDefaults));},registerForms:function(){if(this.formsRegistered){return;}
ul.cmp.ComponentManager.registerCType(ComagicWidget.widget.lead.form.PersonalPanel);ul.cmp.ComponentManager.registerCType(ComagicWidget.widget.lead.form.OfflineMessageForm);ul.cmp.ComponentManager.registerCType(ComagicWidget.widget.lead.form.CallGeneratorPanel);this.formsRegistered=true;},hideForm:function(params){var currentCmp=this.forms[params.log_id];this.hidedFormIds.push(params.log_id);if(!currentCmp||currentCmp.hasBeenSubmitted){return;}
this.closeLeadForm(currentCmp);},trackMouseForShowForm:function(params){if(!g.Comagic.isReady()){return;}
var me=this,lastDate=null,lastY=null,lastV=null,isFormnShown=false,aMin=350;var onMouseMove=function(e){if(isFormnShown){uLiB.$('body').unbind('mousemove',onMouseMove);return;}
var y=e.screenY,windowHeight=window.innerHeight||document.documentElement.clientHeight||document.body.clientHeight||0;if(y>windowHeight*2/3||lastY!==null&&y>=lastY){lastDate=null;lastY=null;lastV=null;return;}
if(lastDate===null){lastDate=+new Date;lastY=y;return;}
var date=+new Date,dateDelta=date-lastDate;if(dateDelta<5){return;}
var moveLength=Math.abs(y-lastY),v=moveLength/dateDelta,vDelta=v-lastV,a=vDelta/dateDelta;if(a*1000>aMin&&u.arrayIndexOf(['lead','invite','in_chat'],g.Comagic.getOCState())===-1){me.onClosePageAttemptDetected(params);isFormnShown=true;}
lastDate=date;lastY=y;lastV=v;};uLiB.$('body').bind('mousemove',onMouseMove);},onClosePageAttemptDetected:function(params){if(u.arrayIndexOf(this.hidedFormIds,params.log_id)===-1){this.showForm(params);this.trackFormShowOnServer(params);}},trackFormShowOnServer:function(params){g.Comagic.trackLeadShow({action_type:params.action_type,action_log_id:params.log_id});},_startCall:function(formCmp){if(formCmp.isValid()){g.Comagic.onLeadFormSubmit(formCmp.s['log_id'],formCmp.getValues(),this.getOnFormSubmitCallback(formCmp.s['log_id']));}},onUlibEvent:function(cmp,deepCType,ctype,$event,target,isTopmostCmp){var formCtype=deepCType.split('--')[0],formCmp=cmp.getParentByCType(formCtype);if(!formCmp||!formCmp.isLeadForm||formCmp.is_preview){return;}
if(deepCType==='call_generator--field'&&$event.type==='keydown'){if($event.keyCode==13){this._startCall(formCmp);}}
if(ctype==='common-button-send'){this._startCall(formCmp);}else if(ctype==='tool-button-close'){if(formCmp.is_preview){return;}
this.closeLeadForm(formCmp);if(!formCmp.hasBeenSubmitted){g.Comagic.onLeadFormCancel(formCmp.s['log_id']);}}},getOnFormSubmitCallback:function(action_log_id){var me=this,form=me.getForm({log_id:action_log_id});if(form.getCallback){return form.getCallback();}else{return ComagicWidget.widget.common.cmp.PopupMessageManager.getCallback(form,me.onPopupMessageClose);}},onPopupMessageClose:function(result,cmp,popupPanel){if(cmp._showModal){cmp._showModal(false);}
cmp.destroy();},closeLeadForm:function(cmp){if(cmp.isModal){cmp.destroy();}else{cmp.show(false);}}};u.apply(u.ns(ns),{Lead:Lead});new Lead();})(window,'uLiB.lead');